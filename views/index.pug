extends layout

block content
  h1= title
  p Welcome to #{title}

  if user
    p=user.username 
    p=user.id 
    p=user.member
    p=user.admin
  else 
    p Not logged in

  each message in messages 
    li
      p #{message.title}
      p #{message.message}
      p #{message.timestamp}
      p #{message.user}